{
	"name": "SFMC_REF_TENANT",
	"properties": {
		"activities": [
			{
				"name": "Copy data to SFMC",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "REF_TENANT EXTRACT",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 3,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFolderPath": {
								"value": "@concat('REF_Tenant',formatDateTime(convertTimeZone(utcNow(),'UTC','China Standard Time'),'yyyyMMdd'))",
								"type": "Expression"
							},
							"wildcardFileName": "*.csv.gz",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureFileStorageWriteSettings",
							"copyBehavior": "MergeFiles"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "TEMP_SFMC_REF_Tenant",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "SHARE_TEMP_SFMC_REF_Tenant",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "REF_TENANT EXTRACT",
				"type": "ExecuteDataFlow",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 3,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataflow": {
						"referenceName": "SFMC_REF_TENANT",
						"type": "DataFlowReference"
					},
					"integrationRuntime": {
						"referenceName": "1crm-IR-TagMgmt-prod",
						"type": "IntegrationRuntimeReference"
					},
					"traceLevel": "Fine"
				}
			}
		],
		"folder": {
			"name": "SFMC"
		},
		"annotations": [],
		"lastPublishTime": "2023-07-27T09:03:55Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}